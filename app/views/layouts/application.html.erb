<!DOCTYPE html>
<html ng-app>
  <head>
    <%= favicon_link_tag 'favicon.ico' %>  
    <title>Oral Exam App | <%= yield(:subtitle) %></title>
      <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
      <%= javascript_include_tag "application", "angular" => true %>
    <%= csrf_meta_tags %>
    <!--[if lt IE 9]>
    <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
    <![endif]-->

  </head>
  <body>
     <%= render 'layouts/header' %>
     <div class='container'>
       <%= yield %>
     </div>
  </body>
</html>